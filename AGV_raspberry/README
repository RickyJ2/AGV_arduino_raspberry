# Run these commands to install the required dependencies

#create env
python3 -m venv venv
source venv/bin/activate

# Install the dependencies from the requirement.txt file
pip install -r requirement.txt

# unpack BreezySLAM.zip
# Change directory to BreezySLAM/python
cd BreezySLAM/python

# Install BreezySLAM
python setup.py install

# unpack PyRoboViz.zip
# Change directory to PyRoboViz
cd PyRoboViz

# Install PyRoboViz
python setup.py install

#pip version 24.0 python 3.11

Change server IP and AGV ID on config.py file

//run this command to ignore update of config.py and rerun this cmd to retrack the changes
git update-index --no-assume-unchanged config.py